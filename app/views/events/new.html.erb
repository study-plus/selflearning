<%= form_with(model: [@user,@event], url: user_events_path, local: true) do |f| %>

    <div class="title">
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>

    <div class="time">
        <%= f.label :start %>
        <%= f.datetime_select :start %>
        <%= f.label :end %>
        <%= f.datetime_select :end %>
    </div>

    <div class="content">
        <%= f.label :content %>
        <%= f.text_field :content %>
    </div>


    <div class="submit">
        <%= f.submit %>
    </div>

<% end %>